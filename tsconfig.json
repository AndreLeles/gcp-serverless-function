{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": true,
    "target": "ES2020",
    "outDir": "./",
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false
  },
  "include": ["src/**/*.ts"],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}